{
  "name": "@createdapp/extension",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development",
    "build:dev": "webpack --mode development",
    "build": "tsc --noEmit && rimraf build && webpack --mode production --progress",
    "lint": "pnpm lint:prettier --check && pnpm lint:eslint",
    "lint:fix": "prettier --write . && pnpm lint:eslint --fix",
    "lint:eslint": "eslint './{src, test}/**/*.{ts,tsx}'",
    "lint:prettier": "prettier --check .",
    "test": "mocha --exit",
    "storybook": "start-storybook -s ./static -p 6006",
    "build-storybook": "build-storybook -s ./static"
  },
  "dependencies": {
    "ethers": "^5.5.1",
    "solid-js": "1.3.10",
    "solid-styled-components": "^0.27.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@storybook/html": "^6.2.9",
    "@types/chai": "^4.2.14",
    "@types/chrome": "^0.0.136",
    "@types/mocha": "^8.2.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^6.2.1",
    "esbuild-loader": "^2.11.0",
    "eslint": "7.19.0",
    "mocha": "^8.2.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  }
}
